<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>AddPatient</servlet-name>
  	<display-name>AddPatient</display-name>
  	<description></description>
  	<servlet-class>aaa.AddPatient</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddHospital</servlet-name>
  	<display-name>AddHospital</display-name>
  	<description></description>
  	<servlet-class>aaa.AddHospital</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>AddPatient</servlet-name>
  	<url-pattern>/AddPatient</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddHospital</servlet-name>
  	<url-pattern>/AddHospital</url-pattern>
  </servlet-mapping>
</web-app>
